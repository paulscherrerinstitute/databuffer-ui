{
	"compilerOptions": {
		"sourceMap": true,
		"module": "es2015",
		"target": "es2017",
		"moduleResolution": "node",
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		// "noUnusedLocals": true,
		// "noUnusedParameters": true,
		"noEmitHelpers": true,
		"importHelpers": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"]
	}
}
